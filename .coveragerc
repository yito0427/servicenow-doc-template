[run]
source = src
omit = 
    src/web/static/*
    src/templates/*.j2
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = ServiceNow Document Generator Coverage Report

[xml]
output = coverage.xml